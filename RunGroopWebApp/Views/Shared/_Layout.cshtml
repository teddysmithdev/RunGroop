     <div class="navbar-container">
      <div class="bg-light navbar-light" data-sticky="top">
        <div class="container">
          <nav class="navbar navbar-expand-lg">
            <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index" id="logo-text">RunGroops</a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
              <i class="icon-menu h4"></i>
            </button>
            <div class="collapse navbar-collapse justify-content-between" id="navbarNav">
              <ul class="navbar-nav">
                  <li class="nav-item">
                            <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        @if (User.Identity.IsAuthenticated)
                        {
                            <li class="nav-item">
                                <a class="nav-link" asp-area="" asp-controller="user" asp-action="Index">Find Runners</a>
                            </li>
                        }
                        <li class="nav-item">
                            <a class="nav-link" asp-area="" asp-controller="Club" asp-action="Index">Find Clubs</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-area="" asp-controller="Race" asp-action="Index">Find Races</a>
                        </li>
                
              </ul>
             @if (User.Identity.IsAuthenticated)
             {
                <ul class="navbar-nav">
                    <li class="nav-item dropdown">
                  <a class="nav-link dropdown-toggle" href="#" id="pagesDropdown" role="button" data-toggle="dropdown">Dashboard</a>
                  <div class="dropdown-menu" aria-labelledby="pagesDropdown">

                    <a class="dropdown-item" href="pages-landing.html">
                      <span class="h6 mb-0">Create Club</span>
                    </a>

                    <div class="dropdown-divider"></div>

                    <a class="dropdown-item" href="pages-app.html">
                      <span class="h6 mb-0">Create Race</span>
                    </a>

                    <div class="dropdown-divider"></div>

                    <a class="dropdown-item" href="pages-inner.html">
                      <span class="h6 mb-0">Edit User Profile</span>
                    </a>
                    <div class="dropdown-divider"></div>
                    <a class="dropdown-item" href="pages-inner.html">
                      <span class="h6 mb-0">Manage Your Clubs/Races</span>
                    </a>
                    <div class="dropdown-divider"></div>
                    <a class="dropdown-item" href="pages-inner.html">
                      <span class="h6 mb-0">Logout</span>
                    </a>
                  </div>
                </li>
                </ul>     
                }
                else
                {
                 <ul class="navbar-nav">
                    <li class="nav-item">
                      <a asp-area="" asp-controller="Account"
                                    asp-action="Register">Sign up</a>
                      <span>&nbsp;or&nbsp;</span><a asp-area="" asp-controller="Account"
                                    asp-action="Login">Sign in</a>
                    </li>
                </ul>
                }

            </div>
            <!--end nav collapse-->
          </nav>
        </div>
        <!--end of container-->
      </div>
    </div>
